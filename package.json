{
  "name": "textr-cli",
  "version": "0.1.0",
  "description": "CLI for Textr",
  "author": {
    "name": "Denys Dovhan",
    "email": "email@denysdovhan.com",
    "url": "denysdovhan.com"
  },
  "bin": {
    "textr": "bin/textr"
  },
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "lint": "eslint cli.js",
    "start": "babel cli.js --watch --out-file cli.es5.js ",
