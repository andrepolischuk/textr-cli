  },
  "homepage": "https://github.com/denysdovhan/textr-cli#readme",
  "devDependencies": {
    "babel-cli": "^6.0.12",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.0.12",
    "eslint": "^1.8.0",
    "npm-run-all": "*",
  },
  "dependencies": {
    "get-stdin": "^5.0.0",
    "minimist": "^1.2.0",
    "textr": "^0.3.0"
